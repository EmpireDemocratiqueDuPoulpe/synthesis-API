{
  "plugins": [
    "plugins/markdown",
    "node_modules/better-docs/category"
  ],
  "recurseDepth": 10,
  "source": {
    "include": [ ],
    "exclude": [ "./.github/", "./node_modules/", "./docs/", "./certs/", "./config/", "./api/routes/" ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "opts": {
    "template": "node_modules/better-docs",
    "encoding": "utf-8",
    "readme": "./README.md",
    "destination": "./docs/www/",
    "recurse": true,
    "verbose": true,
    "private": false
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [ "jsdoc", "closure" ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "search": true,
    "better-docs": {
      "name": "Synthesis API",
      "title": "Synthesis - API",
      "logo": "https://i.imgur.com/ePRsf5Q.png",
      "css": "../custom/top-nav.css",
      "hideGenerator": false,
      "navLinks": [
        { "label": "GitHub", "href": "https://github.com/EmpireDemocratiqueDuPoulpe/synthesis-API" },
        { "label": "YouTube", "href": "https://www.youtube.com/watch?v=dQw4w9WgXcQ" }
      ]
    }
  }
}
