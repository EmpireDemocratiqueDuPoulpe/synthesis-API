{
        "env": {
          "browser": true,
          "es2021": true
        },
        "extends": "google",
        "parserOptions": {
          "ecmaVersio": 12,
          "sourceType": "module"
        },
        "rules": {
          "linebreak-style": "off",
          "no-mixed-spaces-and-tabs": "off",
          "no-tabs": "off",
          "indent": ["off", "tab"],
          "max-len": ["error", { "code": 120}],
          "quotes": ["error", "single", { "avoidEscape": true}]
        }
      }